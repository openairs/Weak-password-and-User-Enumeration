@using WebApplication1.Models
@{
	ViewBag.Title = "UserList";
}
<Center>
	<h1>Here is the List of User On the Sites</h1>
	<table border ="1">
		<tr>
			<th>Id</th>
			<th>First Name</th>
			<th>Last Name</th>
			<th>UserNam</th>
			<th>Password</th>
		</tr>
		@foreach (var item in ViewData["Datalist"] as List<DataClass>)
		{
			<tr>
				<td>@item.Id</td>
				<td>@item.FirstName</td>
				<td>@item.LastName</td>
				<td>@item.UserName</td>
				<td>@item.Password</td>
			</tr>
		}
	</table>
</Center>
